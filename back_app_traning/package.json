{
  "name": "back_app_gym",
  "version": "1.0.0",
  "description": "backEnd for APP GYM",
  "main": "index.js",
  "author": "APP GYM LTDA ",
  "license": "MIT",
  "scripts": {
    "dev": "tsx watch src/server/index.ts",
    "build": "tsup src",
    "start": "node dist/server/index.js",
    "dbmigrate:generate": "npx sequelize migration:generate --name",
    "dbcreate:dev": "npx cross-env NODE_ENV=development sequelize db:create",
    "dbcreate:test": "npx cross-env NODE_ENV=test sequelize db:create",
    "dbcreate:prod": "npx cross-env NODE_ENV=production sequelize db:create",
    "dbmigrate:dev": "npx cross-env NODE_ENV=development sequelize db:migrate",
    "dbmigrate:prod": "npx cross-env NODE_ENV=production sequelize db:migrate",
    "test": "yarn jest --runInBand --no-cache & yarn run undo:all:migrate:test",
    "pretest": "yarn run create:all:migrate:test",
    "test:unit": "yarn jest --testNamePattern='@unit' --runInBand --no-cache & yarn run undo:all:migrate:test",
    "pretest:unit": "yarn run create:all:migrate:test",
    "test:integration": "yarn jest --testNamePattern='@integration' --runInBand --no-cache & yarn run undo:all:migrate:test",
    "pretest:integration": "yarn run create:all:migrate:test",
    "test:groups": "yarn jest --testNamePattern='@unit' --runInBand & yarn jest --testNamePattern='@integration' --runInBand --no-cache & yarn run undo:all:migrate:test",
    "pretest:groups": "yarn run create:all:migrate:test",
    "test:dev": "yarn jest --testNamePattern='@dev' --runInBand --no-cache & yarn run undo:all:migrate:test",
    "pretest:dev": "yarn run create:all:migrate:test",
    "undo:all:migrate:test": "npx cross-env NODE_ENV=test sequelize db:migrate:undo:all",
    "create:all:migrate:test": "npx cross-env NODE_ENV=test sequelize db:migrate"
  },
  "dependencies": {
    "@types/multer": "^1.4.7",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "filesize": "^10.0.12",
    "firebase-admin": "^11.10.1",
    "jsonwebtoken": "^9.0.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.5.2",
    "nodemailer": "^6.9.4",
    "sequelize": "^6.32.1",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.4.2",
    "@types/nodemailer": "^6.4.8",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "eslint": "^8.45.0",
    "jest": "^29.6.1",
    "sequelize-cli": "^6.6.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsup": "^7.1.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6"
  }
}
